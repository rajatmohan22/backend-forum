<%- include('partials/header') %>
  <%- include('partials/navbar') %>

  <div class="container mt-3 mb-3">
    <div class="row">
        <div class="col-6 offset-3">
            <form action="/register" method="post">
                <div class="mb-3">
                  <label for="exampleInputEmail1" class="form-label">Email address</label>
                  <input name="email" placeholder="janedoe@interview.com" type="email" class="form-control" id="email">
                </div>

                <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Username</label>
                    <input name="username" placeholder="janedoe" type="text" class="form-control" id="username">
                  </div>

                  <div class="mb-3">
                    <label for="exampleInputEmail1" class="form-label">Full Name</label>
                    <input name="name" placeholder="Jane Doe" type="text" class="form-control" id="name">
                  </div>

                <div class="mb-3">
                  <label for="exampleInputPassword1" class="form-label">Password</label>
                  <input name="password" type="password" class="form-control" id="password">
                </div>

                <button type="submit" class="btn btn-primary">Submit</button>
              </form>

        </div>
    </div>
  </div>
    
    
      <%- include('partials/footer') %>